            "source": [
                "function migratoryBirds(arr) {\n    let sightings = new Array(5).fill(0); \n    \n     \n     for (let i = 0; i < arr.length; i++) {\n       sightings[arr[i]-1]++; \n       \n     }\n     \n     let maxSightings = Math.max(...sightings); \n     \n     let maxType = sightings.indexOf(maxSightings); \n     \n     \n     return maxType + 1; \n     \n   }"
